{
  "name": "easy-widget",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && yarn serve",
    "serve": "node dist/server/main.js",
    "create:module_federation:config": "node ./scripts/createModuleFederationConfig.js",
    "build": "yarn create:module_federation:config && yarn build:server && yarn build:client",
    "build:client": "rimraf dist/client && npx webpack --mode=production --progress --config webpack/webpack.client.js",
    "build:server": "rimraf dist/server && npx webpack --mode=production --progress --config webpack/webpack.server.js"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "mocha": "^9.2.2",
    "@babel/plugin-syntax-jsx": "^7.16.7",
    "@babel/template": "^7.16.7",
    "@babel/register": "^7.17.7",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.6",
    "chai-enzyme": "^1.0.0-beta.1",
    "coveralls": "^3.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.29.4",
    "istanbul": "^0.4.5",
    "npm-check-updates": "^12.5.3",
    "express": "4.18.2",
    "@babel/core": "7.21.4",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-react": "7.18.6",
    "babel-loader": "^8.2.2",
    "html-webpack-plugin": "^5.3.1",
    "webpack": "^5.38.1",
    "webpack-merge": "5.8.0",
    "webpack-federation-stats-plugin": "1.0.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.0.0",
    "autoprefixer": "^9.3.1",
    "css-loader": "^5.1.1",
    "postcss-loader": "^3.0.0",
    "resolve-url-loader": "^3.1.1",
    "mini-css-extract-plugin": "^1.4.0",
    "prop-types": "^15.8.1",
    "react-helmet": "^6.1.0",
    "style-loader": "3.3.2",
    "@module-federation/node": "0.14.0",
    "json-beautify": "1.1.1",
    "copy-webpack-plugin": "^6.4.1"
  }
